@page "/grid-use-last"
@using FlowSync
@implements IDisposable

<Grid SyncStrategy="SyncStrategy" Header="Grid With 'Use Last' Strategy"></Grid>


@code
{
    readonly IFlowSyncStrategy<int> SyncStrategy = new UseLastCoalescingSyncStrategy<int>();


    void IDisposable.Dispose()
    {
        this.SyncStrategy.CancelAll();
        this.SyncStrategy.Dispose();
    }
}
